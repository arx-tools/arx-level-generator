{
  "name": "arx-level-generator",
  "version": "0.17.0",
  "description": "A tool to generate random Arx Fatalis maps",
  "main": "build/generate.js",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "npm run generate && npm run compile && npm run run",
    "generate": "./scripts/generate.sh",
    "compile": "./scripts/compile.sh",
    "run": "./scripts/run.sh",
    "test:unit": "set FORCE_COLOR=true && mocha -r tsconfig-paths/register -r ts-node/register --timeout 5000 test/**/*.spec.ts",
    "test:unit:watch": "nodemon --exec \"npm run test:unit\" --watch test --watch src -e ts,js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meszaros-lajos-gyorgy/arx-level-generator.git"
  },
  "author": "Lajos Meszaros <m_lajos@hotmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/meszaros-lajos-gyorgy/arx-level-generator/issues"
  },
  "homepage": "https://github.com/meszaros-lajos-gyorgy/arx-level-generator#readme",
  "dependencies": {
    "arx-convert": "^5.0.0",
    "color-rgba": "^2.4.0",
    "nanoid": "^4.0.0",
    "node-pkware": "^3.0.1",
    "seedrandom": "^3.0.5",
    "three": "^0.148.0",
    "tiny-glob": "^0.2.9"
  },
  "devDependencies": {
    "@types/color-rgba": "^2.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.17",
    "@types/seedrandom": "^3.0.3",
    "@types/three": "^0.146.0",
    "arx-header-size": "^2.0.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.10",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "arx-fatalis"
  ]
}
