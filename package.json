{
  "name": "arx-level-generator",
  "version": "0.17.0",
  "description": "A tool for creating Arx Fatalis maps",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "type": "module",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "default": "./build/src/index.js",
      "types": "./build/src/index.d.ts"
    },
    "./prefabs/entity": {
      "default": "./build/src/prefabs/entity/index.js",
      "types": "./build/src/prefabs/entity/index.d.ts"
    },
    "./prefabs/mesh": {
      "default": "./build/src/prefabs/mesh/index.js",
      "types": "./build/src/prefabs/mesh/index.d.ts"
    },
    "./prefabs/rooms": {
      "default": "./build/src/prefabs/rooms/index.js",
      "types": "./build/src/prefabs/rooms/index.d.ts"
    },
    "./scripting": {
      "default": "./build/src/scripting/index.js",
      "types": "./build/src/scripting/index.d.ts"
    },
    "./scripting/classes": {
      "default": "./build/src/scripting/classes/index.js",
      "types": "./build/src/scripting/classes/index.d.ts"
    },
    "./scripting/commands": {
      "default": "./build/src/scripting/commands/index.js",
      "types": "./build/src/scripting/commands/index.d.ts"
    },
    "./scripting/properties": {
      "default": "./build/src/scripting/properties/index.js",
      "types": "./build/src/scripting/properties/index.d.ts"
    },
    "./tools": {
      "default": "./build/src/tools/index.js",
      "types": "./build/src/tools/index.d.ts"
    },
    "./tools/mesh": {
      "default": "./build/src/tools/mesh/index.js",
      "types": "./build/src/tools/mesh/index.d.ts"
    }
  },
  "scripts": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arx-tools/arx-level-generator.git"
  },
  "author": "Lajos Meszaros <m_lajos@hotmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/arx-tools/arx-level-generator/issues"
  },
  "homepage": "https://github.com/arx-tools/arx-level-generator#readme",
  "dependencies": {
    "arx-convert": "^7.0.1",
    "arx-header-size": "^2.1.0",
    "color-rgba": "^2.4.0",
    "dotenv": "^16.0.3",
    "nanoid": "^4.0.2",
    "node-pkware": "^3.0.4",
    "seedrandom": "^3.0.5",
    "sharp": "^0.32.1",
    "sharp-bmp": "^0.1.5",
    "three": "^0.152.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/color-rgba": "^2.1.0",
    "@types/node": "^20.2.5",
    "@types/seedrandom": "^3.0.5",
    "@types/sharp": "^0.31.1",
    "@types/three": "^0.152.1",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "arx-fatalis"
  ]
}
