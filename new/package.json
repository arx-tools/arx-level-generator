{
  "name": "arx-level-generator-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "validate": "tsc src/index.ts --noEmit --skipLibCheck --target esnext --moduleResolution node",
    "build": "esbuild ./src/*.ts ./src/**/*.ts --platform=node --format=cjs --outdir=build",
    "bundle": "esbuild ./src/index.ts --bundle --platform=node --minify --outdir=build --external:./node_modules/*",
    "run": "node --require dotenv/config build/index.js",
    "dev": "npm run validate && npm run build && npm run run"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.39",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "arx-level-json-converter": "^1.3.1",
    "node-pkware": "^1.0.1"
  }
}
